{
  "name": "@croacroa/react-native-template",
  "version": "3.3.0",
  "description": "Production-ready React Native template with Expo, authentication, i18n, offline support, and more",
  "main": "expo-router/entry",
  "author": "Croacroa <contact@croacroa.dev>",
  "license": "MIT",
  "homepage": "https://github.com/croacroa-dev-team/template-react-native#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/croacroa-dev-team/template-react-native.git"
  },
  "bugs": {
    "url": "https://github.com/croacroa-dev-team/template-react-native/issues"
  },
  "keywords": [
    "react-native",
    "expo",
    "template",
    "typescript",
    "nativewind",
    "tailwind",
    "zustand",
    "react-query",
    "authentication",
    "i18n",
    "offline",
    "biometrics",
    "push-notifications",
    "sentry",
    "production-ready"
  ],
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/vector-icons": "~14.0.4",
    "@gorhom/bottom-sheet": "^5.0.6",
    "@hookform/resolvers": "^3.9.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.4.1",
    "@sentry/react-native": "~6.10.0",
    "@shopify/flash-list": "1.7.3",
    "@tanstack/query-async-storage-persister": "^5.66.0",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-persist-client": "^5.66.0",
    "bottleneck": "^2.19.5",
    "burnt": "^0.12.2",
    "clsx": "^2.1.1",
    "expo": "~52.0.49",
    "expo-apple-authentication": "~7.1.3",
    "expo-application": "~6.0.2",
    "expo-auth-session": "~6.0.3",
    "expo-background-fetch": "~13.0.3",
    "expo-camera": "~16.0.18",
    "expo-constants": "~17.0.3",
    "expo-contacts": "~14.0.5",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.8",
    "expo-device": "~7.0.1",
    "expo-font": "~13.0.2",
    "expo-image": "~2.0.4",
    "expo-image-manipulator": "~13.0.6",
    "expo-image-picker": "~16.0.6",
    "expo-linking": "~7.0.4",
    "expo-local-authentication": "~15.0.1",
    "expo-localization": "~16.0.0",
    "expo-location": "~18.0.10",
    "expo-media-library": "~17.0.6",
    "expo-notifications": "~0.29.12",
    "expo-router": "~4.0.15",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.20",
    "expo-sqlite": "~15.1.4",
    "expo-status-bar": "~2.0.1",
    "expo-store-review": "~8.0.1",
    "expo-system-ui": "~4.0.6",
    "expo-task-manager": "~12.0.3",
    "expo-updates": "^0.27.5",
    "expo-web-browser": "~14.0.2",
    "i18next": "^24.2.1",
    "nativewind": "4.1.23",
    "react": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.2.0",
    "react-native": "^0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-react-native-web": "^0.0.24",
    "@storybook/react": "^8.5.0",
    "@storybook/react-webpack5": "^8.5.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.18",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "ajv": "^8.17.1",
    "eslint": "^8.57.1",
    "eslint-config-expo": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.3",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "sharp": "^0.34.5",
    "storybook": "^8.5.0",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "files": [
    "app",
    "assets",
    "components",
    "constants",
    "hooks",
    "i18n",
    "services",
    "stores",
    "types",
    "utils",
    "scripts",
    "maestro",
    "docs",
    ".storybook",
    ".github",
    "__tests__",
    "app.config.ts",
    "babel.config.js",
    "eas.json",
    "global.css",
    "jest.config.js",
    "metro.config.js",
    "nativewind-env.d.ts",
    "tailwind.config.js",
    "tsconfig.json",
    ".env.example",
    ".eslintrc.js",
    ".prettierrc",
    ".gitignore",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
